{
  "name": "create-memberstack-app",
  "version": "0.1.0",
  "description": "Create Memberstacked-powered React apps.",
  "license": "MIT",
  "bin": {
    "create-memberstack-app": "./dist/index.js",
    "create-memberstack": "./dist/index.js"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "copy": "cpy templates ../dist/ --cwd=src",
    "build": "yarn copy && tsup src/index.ts --format cjs --minify",
    "dev": "yarn copy && tsup src/index.ts --format cjs --watch",
    "start": "node dist/index.js",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf node_modules"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^12.6.8",
    "@types/prompts": "2.0.1",
    "@types/rimraf": "3.0.0",
    "@types/tar": "4.0.3",
    "@types/validate-npm-package-name": "3.0.0",
    "async-retry": "1.3.1",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "cpy": "7.3.0",
    "cpy-cli": "^4.1.0",
    "cross-spawn": "6.0.5",
    "got": "10.7.0",
    "prettier": "latest",
    "prompts": "2.1.0",
    "rimraf": "3.0.0",
    "tar": "4.4.10",
    "tsup": "^6.1.2",
    "typescript": "^4.6.4",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.17"
}
